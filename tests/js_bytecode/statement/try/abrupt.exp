}

[BytecodeFunction: testAbruptBodyOnly] {
  Parameters: 0, Registers: 2
     0: Mov r0, <scope>
     3: LoadUndefined r1
     5: Ret r1
     7: Mov <scope>, r0
    10: LoadImmediate r1, 1
    13: LoadUndefined r0
    15: Ret r0
  Exception Handlers:
    3-7 -> 7
}

[BytecodeFunction: testAbruptCatchOnly] {
  Parameters: 0, Registers: 2
     0: Mov r0, <scope>
     3: LoadImmediate r1, 1
     6: Jump 9 (.L0)
     8: Mov <scope>, r0
    11: LoadUndefined r1
    13: Ret r1
  .L0:
    15: LoadUndefined r0
    17: Ret r0
  Exception Handlers:
    3-6 -> 8
}

[BytecodeFunction: testAbruptCatchAndBody] {
  Parameters: 0, Registers: 2
     0: Mov r0, <scope>
     3: LoadUndefined r1
     5: Ret r1
     7: Mov <scope>, r0
